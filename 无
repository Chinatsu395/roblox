local OrionLib = loadstring(game:HttpGet("https://pastebin.com/raw/FUEx0f3G"))()

local Window = OrionLib:MakeWindow({Name = "无脚本", HidePremium = false, SaveConfig = true,IntroText = "无脚本", ConfigFolder = "无脚本"})
game:GetService("StarterGui"):SetCore("SendNotification",{ Title = "无脚本通知"; Text ="禁止外传🚫外传禁止sm"; Duration = 4; })

game:GetService("StarterGui"):SetCore("SendNotification",{ Title = "无脚本通知"; Text ="感谢游玩无脚本"; Duration = 4; })

